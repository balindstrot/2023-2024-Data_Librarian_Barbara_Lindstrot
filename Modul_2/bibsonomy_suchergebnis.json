     ],\n            \"authors\": [\n                {\n                    \"first\": \"Patrick\",\n                    \"last\": \"Danowski\"\n                },\n                {\n                    \"first\": \"Jakob\",\n                    \"last\": \"Vo\\u00df\"\n                }\n            ],\n            \"volume\": \"55\",\n            \"number\": \"8\",\n            \"pages\": \"457ff.\",\n            \"bibtexKey\": \"danowski2004bibliothek\"\n        },\n        {\n            \"type\": \"Publication\",\n            \"id\": \"https://www.bibsonomy.org/bibtex/22596e5c729e3ad3cf1480bc74fe68d63/genealogie\",\n            \"tags\": [\n                \"dnb\",\n                \"Bibliotheks_und_Informationswissenschaft\"\n            ],\n            \"intraHash\": \"2596e5c729e3ad3cf1480bc74fe68d63\",\n            \"interHash\": \"fb6faeeed2ffb1102678a7491e0ca2be\",\n            \"label\": \"Die Bibliothek als Speichersystem des kulturellen Ged\\u00e4chtnisses\",\n            \"user\": \"genealogie\",\n            \"description\": \"\",\n            \"date\": \"2015-09-10 02:59:23\",\n            \"changeDate\": \"2015-09-10 02:59:23\",\n            \"count\": 1,\n            \"pub-type\": \"phdthesis\",\n            \"year\": \"2009\",\n            \"url\": \"\",\n            \"author\": [\n                \"Tanja Heber\"\n            ],\n            \"authors\": [\n                {\n                    \"first\": \"Tanja\",\n                    \"last\": \"Heber\"\n                }\n            ],\n            \"dnbtitleid\": \"996362622\",\n            \"bibtexKey\": \"heber2009bibliothek\"\n        },\n        {\n            \"type\": \"Publication\",\n            \"id\": \"https://www.bibsonomy.org/bibtex/280ee1dfb8297fda74dc6078da1aff955/thapke\",\n            \"tags\": [\n                \"imported\",\n                \"Hapke_publication\"\n            ],\n            \"intraHash\": \"80ee1dfb8297fda74dc6078da1aff955\",\n            \"interHash\": \"00a19c69d47ed3e403ab7c42e20bcd41\",\n            \"label\": \"Die Bibliothek der Sielkl\\u00e4r-Versuchsstation Hamburg-Eppendorf\",\n            \"user\": \"thapke\",\n            \"description\": \"\",\n            \"date\": \"2010-05-12 16:51:59\",\n            \"changeDate\": \"2010-05-12 16:52:05\",\n            \"count\": 1,\n            \"pub-type\": \"article\",\n            \"journal\": \"Stadthygiene und Abwasserreinigung nach der Hamburger Cholera-Epidemie . - Herzberg : Bautz\",\n            \"year\": \"1993\",\n            \"url\": \"\",\n            \"author\": [\n                \"Thomas Hapke\"\n            ],\n            \"authors\": [\n                {\n                    \"first\": \"Thomas\",\n                    \"last\": \"Hapke\"\n                }\n            ],\n            \"pages\": \"62-67\",\n            \"bibtexKey\": \"Hapke.1993d\"\n        },\n        {\n            \"type\": \"Publication\",\n            \"id\": \"https://www.bibsonomy.org/bibtex/292db8ac3e040c811023295bab880b7de/dasboogie\",\n            \"tags\": [\n                \"imported\"\n            ],\n            \"intraHash\": \"92db8ac3e040c811023295bab880b7de\",\n            \"interHash\": \"2ddda56e6e978a61e295e3258ef181cb\",\n            \"label\": \"Reclam Universal-Bibliothek, Nr.12, Wilhelm Tell\",\n            \"user\": \"dasboogie\",\n            \"description\": \"\",\n            \"date\": \"2011-02-03 18:09:31\",\n            \"changeDate\": \"2011-02-03 18:09:31\",\n            \"count\": 1,\n            \"pub-type\": \"book\",\n            \"publisher\": \"Reclam, Ditzingen\",\n            \"year\": \"2000\",\n            \"url\": \"\",\n            \"author\": [\n                \"Friedrich von Schiller\"\n            ],\n            \"authors\": [\n                {\n                    \"first\": \"Friedrich\",\n                    \"last\": \"von Schiller\"\n                }\n            ],\n            \"timestamp\": \"2010-06-14 07:55:23\",\